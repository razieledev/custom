        <!-- this Sequences call when you create MO in formula -->
        <record id="seq_type_lp_mo" model="ir.sequence.type">
            <field name="name">Lp Mo Sequence</field>
            <field name="code">lp.mo.sequence</field>
        </record>

        <record id="seq_lp_mo_formula" model="ir.sequence">
            <field name="name">Lp Mo Sequence</field>
            <field name="code">lp.mo.sequence</field>
            <field name="prefix">LP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
